<%- include("partials/header") -%>

<h1>Home</h1>
<p> <%= content %> </p>
<% posts.forEach(function(elem) { %>
<h1> <%= elem.t %> </h1>
<% let postContent = elem.m %>
<% if(elem.m.length > 100) { postContent = elem.m.substring(0,100) + "..." } %>
<p> <%= postContent %> <a href=<%= elem.path %> >Read more</a> </p>
<% }); %>
<%- include("partials/footer") -%>
