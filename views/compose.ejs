<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
	<label for="title">Title</label><br>
	<input type="text" name="post_title" id="title" class="form-control"><br>
	<label for="post">Post</label><br>
	<textarea name="post_message" id="post" class="form-control" rows="3"></textarea>
	<button type="submit" name="button" class="btn btn-primary com-btn">Publish</button>
</form>

<%- include("partials/footer") -%>
